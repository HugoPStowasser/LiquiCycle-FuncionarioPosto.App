<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LiquiCycle_FuncionarioPosto.Views.App.AddLiquido"
             Title="AddLiquido"
             NavigationPage.HasNavigationBar="False">
    <VerticalStackLayout>
        <Grid Margin="20,20,0,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />

            </Grid.ColumnDefinitions>
            <Label Text="Adicionar liquido" FontSize="24" HorizontalOptions="Center"/>
        </Grid>

        <Grid x:Name="gridLiquidos" RowDefinitions="*,*" Margin="20,20,0,0">
        </Grid>

        <Grid>
            <Button Text="Confirmar" Clicked="Return"/>
        </Grid>
    </VerticalStackLayout>
</ContentPage>